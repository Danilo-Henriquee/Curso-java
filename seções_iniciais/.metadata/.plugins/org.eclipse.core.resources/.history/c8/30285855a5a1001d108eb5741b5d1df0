package entities;

public class Student {
	public String name;
	public double note1, note2, note3, result;
	
	public void noteCalculation() {
		result = note1 + note2 + note3;
	}
	
	public String verification(double result) {
		if (result >= 60) {
			return "PASS";
		}
		else {
			return "FAILED";
		}
	}
}
