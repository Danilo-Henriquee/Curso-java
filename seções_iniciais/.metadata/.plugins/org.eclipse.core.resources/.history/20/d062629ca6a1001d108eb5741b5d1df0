package entities;

public class Student {
	public String name;
	public double note1, note2, note3, result;
	
	public double noteCalculation() {
		return result = note1 + note2 + note3;
	}
	public void show() {
		if (result >= 60) {
			System.out.printf("FINAL GRADE = %.2f%n", result);
			System.out.print("PASS");
		} else {
			System.out.printf("FINAL GRADE = %.2f%n", result);
			System.out.print("FAILED");
			System.out.printf("MISSING %.2f POINTS", result - 100);
		}
	}
}
